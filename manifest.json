{
   "background": {
      "service_worker": "main.js"
   },
   "description": "Enhanced Keyboard Controls for players on streaming services. Currently supports Disney+ and HBO Max.",
   "host_permissions": [ "https://*.disneyplus.com/*", "https://*.hbomax.com/*" ],
   "icons": {
      "128": "logo/StreamKeys_128.png",
      "16": "logo/StreamKeys_16.png",
      "32": "logo/StreamKeys_32.png",
      "48": "logo/StreamKeys_48.png"
   },
   "manifest_version": 3,
   "name": "Stream Keys - Video Keyboard Controls",
   "options_page": "settings/settings.html",
   "permissions": [ "webNavigation", "scripting", "storage" ],
   "version": "1.0.0"
}
