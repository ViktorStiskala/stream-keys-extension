    "eslint-config-prettier": "^9.1.0",
    "jsdom": "^27.3.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0",
    "vite": "^6.0.3",
    "vite-plugin-web-extension": "^4.4.3",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "webextension-polyfill": "^0.12.0"
